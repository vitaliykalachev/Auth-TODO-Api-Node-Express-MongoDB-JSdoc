{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": ["src"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "src/docs",
        "recurse": true,
        "verbose": true,
        "tutorials": "./tutorials"
        
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    }
    
    
}
