{
    "version": 2,

    "rewrites": [{ 
        "source": "./(.*)",
        "destination": "./" 
        }],

    "builds": [
      {
        "src": "/src/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "docs/index.html",
        "use": "@vercel/static",
        "config": { "includeFiles": ["docs/**"] }
      }
    ],
    "routes": [
      {
        "src": "/src/(.*)",
        "dest": "/src/authRouter.js"
      }
    ]
    
   
      
  }