{
    "version": 2,

    "rewrites": [{ 
        "source": "/src/(.*)",
        "destination": "/src" 
        }],

    "builds": [
      {
        "src": "/src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/src/(.*)",
        "dest": "/src/authRouter.js"
      }
    ]
    
   
      
  }