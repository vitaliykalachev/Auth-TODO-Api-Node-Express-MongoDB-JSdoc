{
    "version": 2,

    "rewrites": [{ 
        "source": "./(.*)",
        "destination": "./" 
        }],

    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
      
    ],
    "routes": [
      {
        "src": "src/(.*)",
        "dest": "src/authRouter.js"
      }
    ]
  }